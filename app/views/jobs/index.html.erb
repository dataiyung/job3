<div class="pull-left">
  <%= link_to("新增紅娘", new_job_path, :class => "btn btn-default") %>

</div>
<table class="table table-bordered">
  <% @jobs.each do |job| %>
  <tr>
  <td>
    <%= link_to(job.title, job_path(job)) %>
  </td>
  <td>
    <%=link_to("編輯",edit_job_path(job)) %> |
    <%=link_to("刪除", job_path(job), :method => :delete, :data => {:confirm =>"Are you sure?"})%>
  </td>
  <td>
      <%= job.created_at %>
  </td>
  </tr>
  <% end %>
</table>
